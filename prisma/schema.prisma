// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  email String @id
  password String
  role     String
  submissions Submission[]
}

model Object {
  uuid String @id
  url  String
  submissions Submission[]
}

model Submission {
  quarter    Int
  uuid       Int @id
  year       Int
  uid        String
  objectId String
  status     String

  user   User  @relation(fields: [uid], references: [email])
  object Object @relation(fields: [objectId], references: [uuid])
}

model Date {
  quarter   Int    @id
  startDate String
  endDate   String
}
